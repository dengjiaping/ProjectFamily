                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:columnWidth="85dp"
                android:stretchMode="columnWidth"
                android:numColumns="auto_fit" >
            </GridView>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>