<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/cx_fa_widget_calendar_cell_landspace_fl"
        android:layout_width="65dip"
        android:layout_height="52dip"
        android:background="@null" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dip"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_marginLeft="5dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom" >

                    <TextView
                        android:id="@+id/cx_fa_widget_calendar_cell_landspace_date_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1"
                        
                        android:textColor="#37322f"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/cx_fa_widget_calendar_cell_landspace_date_tv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="29"
                        android:textColor="#37322f"
                        android:textSize="15sp" />
                </FrameLayout>

                <FrameLayout
                    android:layout_marginLeft="1dip"
                    android:layout_gravity="bottom"
                    android:layout_marginBottom="3dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/cx_fa_widget_calendar_cell_lunar_landspace_date_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="初八"
                        android:textColor="#99000000"
                        android:textSize="9sp" />

                    <TextView
                        android:id="@+id/cx_fa_widget_calendar_cell_lunar_landspace_date_tv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="腊八节"
                        android:textColor="#99000000"
                        android:textSize="9sp" />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/cx_fa_calendar_landspace_items_layout"
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"              
                android:orientation="horizontal" >

                
                <ImageView
                    android:id="@+id/cx_fa_widget_calendar_cell_dot_landspace_iv1"
                    android:layout_width="3dip"
                    android:layout_height="3dip"
                    android:layout_margin="2dip"
                    android:src="@drawable/cx_fa_widget_calendar_cell_dot_memorial" />

                <TextView
                    android:id="@+id/cx_fa_widget_calendar_cell_text_landspace_tv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="百日依山尽黄河入海流"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textColor="#696969"
                    android:textSize="7sp" />
                
            </LinearLayout>

        
            <LinearLayout
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/cx_fa_widget_calendar_cell_dot_landspace_iv2"
                    android:layout_width="3dip"
                    android:layout_height="3dip"
                    android:layout_margin="2dip"
                    android:src="@drawable/cx_fa_widget_calendar_cell_dot_item" />

                <TextView
                    android:id="@+id/cx_fa_widget_calendar_cell_text_landspace_tv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="百日依山尽黄河入海流"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="5dip"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/cx_fa_widget_calendar_cell_dot_landspace_iv3"
                    android:layout_width="3dip"
                    android:layout_height="3dip"
                    android:layout_margin="2dip"
                    android:src="@drawable/cx_fa_widget_calendar_cell_dot_item" />

                <TextView
                    android:id="@+id/cx_fa_widget_calendar_cell_text_landspace_tv3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="百日依山尽黄河入海流"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>
      
        </LinearLayout>

        <LinearLayout
 			android:layout_gravity="right"           
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <ImageView
                android:id="@+id/cx_fa_widget_calendar_cell_triangle_landsapce_iv"
                android:layout_width="12dip"
                android:layout_height="12dip"
                android:src="@drawable/calendar_triangle" />
        </LinearLayout>
    </FrameLayout>

</LinearLayout>